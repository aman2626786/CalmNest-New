{
  "buildCommand": "cd CalmNest && npm install && npm run build",
  "outputDirectory": "CalmNest/out",
  "installCommand": "cd CalmNest && npm install",
  "framework": "nextjs",
  "functions": {
    "CalmNest/src/pages/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}